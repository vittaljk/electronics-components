{
  "kind": "collectionType",
  "collectionName": "components",
  "info": {
    "singularName": "component",
    "pluralName": "components",
    "displayName": "Component",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "modelNumber": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "eolDate": {
      "type": "date"
    },
    "isObsolete": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "component"
    },
    "replacableComponents": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::component.component",
      "inversedBy": "components"
    },
    "components": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::component.component",
      "mappedBy": "replacableComponents"
    }
  }
}
